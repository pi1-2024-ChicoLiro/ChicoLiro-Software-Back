generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model dados {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  TrilhaID    String @db.ObjectId
  rpmMotorDir Int?
  rpmMotorEsq Int?
  tensao      Int?
}

model trilha {
  id                  String    @id @default(auto()) @map("_id") @db.ObjectId
  endMovingDatetime   DateTime? @db.Date
  isMoving            Boolean
  name                String
  startMovingDatetime DateTime  @db.Date
}
