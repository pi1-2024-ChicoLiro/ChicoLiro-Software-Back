generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model trilha {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String
  isMoving Boolean
  startMovingDatetime DateTime
  endMovingDatetime DateTime?
}

model dados {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  TrilhaID String @db.ObjectId
  rpm Int
  infraVermelhoEsq Int
  infraVermelhoMeio Int
  infraVermelhoDird Int
  tensao Int
}
